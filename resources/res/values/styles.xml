<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.WallpaperPicker">
    </style>
    <style name="Theme">
    </style>
    <style name="ClingButton">
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/cling_button_bg</item>
        <item name="android:paddingLeft">35px</item>
        <item name="android:paddingTop">10px</item>
        <item name="android:paddingRight">35px</item>
        <item name="android:paddingBottom">15px</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/cling_dismiss</item>
    </style>
    <style name="ClingTitleText">
        <item name="android:textSize">23px</item>
        <item name="android:textColor">#49c0ec</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">5px</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">2</item>
    </style>
    <style name="ClingText">
        <item name="android:textSize">15px</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">2</item>
        <item name="android:lineSpacingMultiplier">1.1</item>
    </style>
    <style name="WorkspaceIcon">
        <item name="android:textSize">@dimen/workspace_icon_text_size</item>
        <item name="android:textColor">@color/workspace_icon_text_color</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#b0000000</item>
        <item name="android:shadowRadius">2</item>
    </style>
    <style name="WorkspaceIcon.Portrait">
        <item name="android:paddingLeft">8px</item>
        <item name="android:paddingTop">@dimen/app_icon_padding_top</item>
        <item name="android:paddingRight">8px</item>
        <item name="android:paddingBottom">8px</item>
        <item name="android:drawablePadding">@dimen/app_icon_drawable_padding</item>
    </style>
    <style name="WorkspaceIcon.Landscape">
        <item name="android:paddingLeft">@dimen/app_icon_padding_left</item>
        <item name="android:paddingTop">@dimen/app_icon_padding_top</item>
        <item name="android:paddingRight">@dimen/app_icon_padding_right</item>
        <item name="android:paddingBottom">@dimen/app_icon_padding_bottom</item>
        <item name="android:drawablePadding">@dimen/app_icon_drawable_padding_land</item>
    </style>
    <style name="WorkspaceIcon.Portrait.Folder">
        <item name="android:paddingTop">@dimen/folder_icon_padding_top</item>
        <item name="android:drawablePadding">0px</item>
    </style>
    <style name="WorkspaceIcon.Landscape.Folder">
        <item name="android:paddingTop">0px</item>
        <item name="android:layout_marginTop">-2px</item>
        <item name="android:drawablePadding">0px</item>
    </style>
    <style name="WorkspaceIcon.Portrait.AppsCustomize">
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">0px</item>
        <item name="android:paddingTop">0px</item>
        <item name="android:paddingRight">0px</item>
        <item name="android:paddingBottom">0px</item>
        <item name="android:maxLines">2</item>
        <item name="android:singleLine">false</item>
        <item name="android:drawablePadding">@dimen/app_icon_drawable_padding</item>
    </style>
    <style name="WorkspaceIcon.Landscape.AppsCustomize">
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">@dimen/apps_customize_icon_padding_left</item>
        <item name="android:paddingTop">@dimen/apps_customize_icon_padding_top</item>
        <item name="android:paddingRight">@dimen/apps_customize_icon_padding_right</item>
        <item name="android:paddingBottom">@dimen/apps_customize_icon_padding_bottom</item>
        <item name="android:drawablePadding">@dimen/app_icon_drawable_padding_land</item>
    </style>
    <style name="QSBBar">
        <item name="android:layout_gravity">left|right|center_vertical|center_horizontal|fill_horizontal|center|clip_horizontal</item>
        <item name="android:orientation">@null</item>
        <item name="android:paddingLeft">@dimen/qsb_padding_left</item>
        <item name="android:paddingRight">@dimen/qsb_padding_right</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/qsb_bar_height</item>
    </style>
    <style name="SearchDropTargetBar">
        <item name="android:layout_gravity">top|left|center_vertical|center_horizontal|center</item>
        <item name="android:orientation">@null</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="SearchButton">
        <item name="android:layout_gravity">clip_horizontal</item>
        <item name="android:paddingTop">@dimen/toolbar_button_vertical_padding</item>
        <item name="android:paddingBottom">@dimen/toolbar_button_vertical_padding</item>
    </style>
    <style name="DropTargetButtonContainer">
        <item name="android:layout_width">0px</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="DropTargetButton">
        <item name="android:textSize">24px</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">clip_horizontal</item>
        <item name="android:layout_gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:paddingLeft">50px</item>
        <item name="android:paddingRight">50px</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">@null</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">4</item>
        <item name="android:drawablePadding">15px</item>
    </style>
    <style name="TabIndicator">
        <item name="android:textSize">24px</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">left|center_horizontal|clip_horizontal</item>
        <item name="android:background">@drawable/tab_widget_indicator_selector</item>
        <item name="android:paddingLeft">40px</item>
        <item name="android:paddingRight">40px</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="TabIndicator.AppsCustomize">
    </style>
    <style name="MarketButton">
        <item name="android:textSize">18px</item>
        <item name="android:textColor">@color/workspace_all_apps_and_delete_zone_text_color</item>
        <item name="android:paddingLeft">10px</item>
        <item name="android:paddingRight">10px</item>
        <item name="android:shadowColor">@color/workspace_all_apps_and_delete_zone_text_shadow_color</item>
        <item name="android:shadowDx">@null</item>
        <item name="android:shadowDy">@null</item>
        <item name="android:shadowRadius">2</item>
    </style>
    <style name="CustomClingTitleText">
    </style>
    <style name="CustomClingText">
    </style>
</resources>
